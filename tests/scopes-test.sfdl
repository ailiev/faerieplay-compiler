program ScopesTest
{

    const WordSize = 8;
    type Word = Int<WordSize>;

    function Word sfdlmain (Word a, Word b)
    {

	var Word [32] A;
	
	if (a > b) {
	    A[12] = 30;
	}
/* 	else { */
