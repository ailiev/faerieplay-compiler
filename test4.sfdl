// -*- c -*-

program Test4 {

	const DIM = 16;

	const logN = 16;
	const N    = (1 << logN);
	
    type Key = Int<16`>;
	type Short = Int<16`>;
	
    const PI = 4;
    
    type Point = struct {
		Short x,
		Short y
    };

    type Input = struct {
		Point center,
		Point[DIM] diagonals
	};

    function Key f (Key x) {
        f = x * 2;
    };



    function Point main (Input in) {
        var Key k2, k3, k4;
		var Point magic;
		
        var Point pt;

		pt = in.center;

		k2 = pt.x;

        k3 = 0;
	
        pt.x = 47;

		magic = in.diagonals[7];

        if (pt.x < in.center.x) {
            for (i = 1 to 3) {
                k3 = f(k3);
            }

            pt.y = k3 * 3;
			pt.x = k2 - 3;
        }

		magic = pt;

/*	
	else {
	    k3 = k;
	    for (i = 1 to 5)
		k3 = f(k3);
	}
*/
		k3 = pt.x + 3;
	
		in.diagonals[2].y = 3;

		main = in.diagonals[2];
		
        main.x = magic.y + magic.x;
		main.y = 2;

		

    }
    

}
